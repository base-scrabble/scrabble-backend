{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    },
    "server.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}