{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.15.10",
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}